{
  "name": "virtualized-table",
  "version": "0.0.1",
  "main": "dist/index.js",
  "module": "es6/index.js",
  "jsnext:main": "es6/index.js",
  "sideEffects": [
    "./src/js/contexts/ThemeContext/ThemeContext.js",
    "./es6/contexts/ThemeContext/ThemeContext.js"
  ],
  "description": "a reusable componenet for changelog panel",
  "bugs": "http://tree.mn/srikanth.p/virtualized-table/issues",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "http://tree.mn/srikanth.p/virtualized-table.git"
  },
  "engine-strict": true,
  "engines": {
    "node": ">= 8"
  },
  "scripts": {
    "build": "webpack --mode production && babel ./src/js/ --ignore '__tests__' --out-dir ./dist --copy-files && cross-env BABEL_ENV=es6 babel ./src/js/ --ignore '__tests__' --out-dir ./dist/es6 --copy-files",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
    "test": "npm run generate-readme && jest --runInBand",
    "test-update": "npm run generate-readme && jest --updateSnapshot",
    "test-watch": "jest --watchAll",
    "test-bundlesize": "bundlesize",
    "pack": "babel-node ./tools/pack",
    "storybook": "cross-env NODE_ENV=development start-storybook -p 9001 -c storybook -s ./storybook/public",
    "build-storybook": "build-storybook -c storybook -o storybook-static -s ./storybook/public",
    "prettier": "pretty-quick --staged",
    "generate": "plop"
  },
  "peerDependencies": {
    "react": ">= 16.6.1",
    "react-dom": ">= 16.6.1",
    "styled-components": ">= 4.X"
  },
  "dependencies": {
    "@testing-library/dom": "^6.1.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.1.3",
    "css": "^2.2.3",
    "hoist-non-react-statics": "^3.2.0",
    "markdown-to-jsx": "^6.9.1",
    "mnet-ui-base": "mnet-ui/mnet-ui-base#release",
    "polished": "^3.4.1",
    "prop-types": "^15.7.2",
    "react-desc": "^4.1.2",
    "react-virtualized": "^9.21.2",
    "recompose": "^0.30.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.3.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@emotion/core": "^10.0.7",
    "@storybook/addon-options": "~5.1.11",
    "@storybook/addon-storysource": "~5.1.11",
    "@storybook/addons": "~5.1.11",
    "@storybook/react": "~5.1.11",
    "@types/node": "^12.11.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/storybook__react": "^4.0.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.0-beta.6",
    "babel-plugin-styled-components": "^1.6.4",
    "babel-plugin-transform-imports": "^2.0.0",
    "bundlesize": "^0.18.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.1.6",
    "del": "^5.1.0",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.9.1",
    "eslint-plugin-react-hooks": "^2.2.0",
    "fs-extra": "^8.1.0",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "jest-styled-components": "^6.2.0",
    "jsdom": "^15.1.1",
    "plop": "^2.5.3",
    "pre-commit": "^1.2.2",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.8.0",
    "react-dev-utils": "^9.0.3",
    "react-test-renderer": "^16.8.3",
    "regenerator-runtime": "^0.13.1",
    "require-reload": "^0.2.2",
    "simple-git": "^1.95.0",
    "storybook-chromatic": "^2.2.2",
    "tarball-extract": "^0.0.6",
    "typescript": "^3.6.4",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.2.3"
  },
  "jest": {
    "collectCoverage": true,
    "testURL": "http://localhost/",
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/js/components/**/*.{js}",
      "src/js/contexts/**/*.{js}",
      "src/js/themes/**/*.{js}",
      "src/js/utils/**/*.{js}",
      "!src/js/components/**/*.{stories.js}",
      "!src/js/components/**/stories/*.js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](dist|node_modules)[/\\\\]"
    ]
  },
  "pre-commit": [
    "lint-fix",
    "prettier",
    "test"
  ],
  "bundlesize": [
    {
      "path": "./dist/mnetreportingui.min.js",
      "maxSize": "120 kB"
    }
  ],
  "keywords": [
    "component",
    "library",
    "react",
    "atomic design",
    "ui",
    "user interface"
  ]
}
